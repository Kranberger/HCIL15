{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "dotnet build (mylogger)",
      "type": "shell",
      "command": "dotnet build ./mylogger",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": "$msCompile",
      "presentation": {
        "reveal": "always",
        "panel": "shared"
      }
    },
    {
      "label": "dotnet run (mylogger)",
      "type": "shell",
      "command": "dotnet run --project ./mylogger",
      "group": "none",
      "presentation": {
        "reveal": "always",
        "panel": "shared",
        "clear": false
      }
    }
  ]
}
